public class Text{
	public static void main(String[] args){
		int a = 0;
		int b = 0;
		int c = 0;
		int d = 0;
		
		for(; a<=8;a++){
			b = 8 - a;
			c = 14 - a;
			d = 2 + a;
			if(c - d == 6){
				System.out.println("a="+a);
				System.out.println("b="+b);
				System.out.println("c="+c);
				System.out.println("d="+d);
			}
		}
	}
}
/*碰到问题尽量简化，能不用多重循环就不用多重循环*/
